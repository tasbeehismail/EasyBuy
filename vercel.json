{
    "version": 2,
    "name": "easybuy",
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "memory": 512,
        "maxDuration": 30
      }
    }
  }